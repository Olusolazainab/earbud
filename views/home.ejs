    <div class="container mt-5">
    <div class="row d-grid gap-2 d-sm-flex  justify-content-sm-center">
      <div class="col-sm-8 form-body">
        <div class="card">
          <div class="card-body">

            <!-- Makes POST request to /register route -->
            <form action="/submit" method="POST" id="form">
              <div class="form-group">
                <label for="name">Name</label>
                <input type="text" class="form-control feedback-input" name="name" placeholder="Name" required>
              </div>
              <div class="form-group">
                <label for="phone">Phone Number</label>
                <input type="number" class="form-control feedback-input" name="phone" placeholder="Phone Number" required>
              </div>
              <div class="form-group">
                <label for="address">Address</label>
                <input type="text" class="form-control feedback-input" name="address" placeholder="Address" required>
              </div>
              <div class="form-group">
                <label for="state">State</label>
                <input type="text" class="form-control feedback-input" name="state" placeholder="State" required>
              </div>
              <div class="form-group">
                <label for="whatsapp">Whatsapp Number </label>
                <input type="number" class="form-control feedback-input" name="whatsapp" placeholder="Whatsapp Number" required>
              </div>
                <div class="form-group">
                  <label for="state">What's your preffered earphone colour?</label>
                  <input type="text" class="form-control feedback-input" name="earbud_colour" placeholder="Black or White" required>
                </div>
                <div class="form-group">
                  <label for="state">What's your preffered fan colour?</label>
                  <input type="text" class="form-control feedback-input" name="fan_colour" placeholder="Green or White" required>
                </div>
                <div class="form-group">
                  <label for="comment">Comment</label>
                  <textarea type="text" class="form-control feedback-input" name="comment" placeholder="Any other Comment?"></textarea>
                </div>
              <div class="d-grid gap-2 d-sm-flex justify-content-sm-center mt-3">
              <button type="submit" class="btn bg">Login</button>
            </div>
            </form>
